
<div class="row-fluid">
	<div class="center span12">
		<h1>You've got feedback. Now tell 'em!</h1>
	</div>
</div>
<div class="row-fluid">
	<div class="span4">&nbsp;</div>
	<div class="left span4">
		
		<%= form_for(@feedback, :html => { :class => "well form_well"} ) do |f| %>

	      <%= f.label :email, 'Feedback for:' %>
	      <%= f.text_field :email, :placeholder => "co-worker@example.com" %>
	
	      <%= f.label :message, 'Message:' %>
	      <%= f.text_area :message, :rows => "3" %>

	      <%= f.submit "Submit", :class => "btn btn-large btn-primary" %>
	    <% end %>
	</div>	
</div>
